{
    "editor.fontFamily": "'Anonymous','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.accessibilitySupport": "off",
    "editor.mouseWheelZoom": true,
    "editor.roundedSelection": false,
    "editor.tabCompletion": "on",
    "editor.formatOnPaste": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "search.smartCase": true,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "uxterm",
    "telemetry.enableTelemetry": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    "telemetry.enableCrashReporter": false,
    "extensions.autoUpdate": false,
    "git.defaultCloneDirectory": "~/projs/",
    "window.zoomLevel": 2,
    "azure.tenant": "",
    "omnisharp.path": "latest",
    "go.autocompleteUnimportedPackages": true,
    "update.mode": "none",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "wal.tokenColorTheme": "Default Dark+",
    "workbench.colorCustomizations": {
        "[Wal]": {
            "focusBorder": "#387ca0",
            "foreground": "#eeeeec",
            "widget.shadow": "#999998",
            "selection.background": "#c6261e",
            "descriptionForeground": "#236e96",
            "errorForeground": "#c6261e",
            "input.background": "#2e2e2e",
            "input.foreground": "#fafcd8",
            "input.placeholderForeground": "#999998",
            "inputOption.activeBorder": "#387ca0",
            "inputValidation.errorBackground": "#c6261e",
            "inputValidation.errorBorder": "#d1514a",
            "inputValidation.infoBackground": "#236e96",
            "inputValidation.infoBorder": "#387ca0",
            "inputValidation.warningBackground": "#f3bc2e",
            "inputValidation.warningBorder": "#f6d06c",
            "list.invalidItemForeground": "#c6261e",
            "list.errorForeground": "#c6261e",
            "list.warningForeground": "#f3bc2e",
            "terminal.background": "#2e2e2e",
            "terminal.foreground": "#eeeeec",
            "terminalCursor.background": "#2e2e2e",
            "terminalCursor.foreground": "#eeeeec",
            "terminal.ansiBlack": "#2e2e2e",
            "terminal.ansiRed": "#c6261e",
            "terminal.ansiGreen": "#c5d86d",
            "terminal.ansiYellow": "#f3bc2e",
            "terminal.ansiBlue": "#236e96",
            "terminal.ansiMagenta": "#d45b12",
            "terminal.ansiCyan": "#97d3db",
            "terminal.ansiWhite": "#fafcd8",
            "terminal.ansiBrightBlack": "#999998",
            "terminal.ansiBrightRed": "#d1514a",
            "terminal.ansiBrightGreen": "#d0df8a",
            "terminal.ansiBrightYellow": "#f6d06c",
            "terminal.ansiBrightBlue": "#387ca0",
            "terminal.ansiBrightMagenta": "#dc7b41",
            "terminal.ansiBrightCyan": "#c0e4e9",
            "terminal.ansiBrightWhite": "#fcfde7",
            "activityBar.background": "#2e2e2e",
            "activityBar.foreground": "#eeeeec",
            "sideBarSectionHeader.background": "#2e2e2e",
            "sideBar.background": "#2e2e2e",
            "sideBar.foreground": "#eeeeec",
            "statusBar.background": "#d45b12",
            "statusBar.foreground": "#fafcd8",
            "statusBar.debuggingBackground": "#c5d86d",
            "statusBar.noFolderBackground": "#c5d86d",
            "tab.activeBackground": "#2e2e2e",
            "tab.activeForeground": "#eeeeec",
            "editor.background": "#2e2e2e",
            "editorOverviewRuler.modifiedForeground": "#f3bc2e",
            "editorOverviewRuler.addedForeground": "#c5d86d",
            "editorOverviewRuler.deletedForeground": "#c6261e",
            "editorOverviewRuler.warningForeground": "#f3bc2e",
            "editorOverviewRuler.infoForeground": "#236e96",
            "editorOverviewRuler.errorForeground": "#c6261e",
            "editorGutter.background": "#2e2e2e",
            "editorGutter.modifiedBackground": "#f3bc2e",
            "editorGutter.addedBackground": "#c5d86d",
            "editorGutter.deletedBackground": "#c6261e",
            "editorError.foreground": "#c6261e",
            "editorWarning.foreground": "#f3bc2e",
            "editorInfo.foreground": "#236e96",
            "editorHint.foreground": "#c5d86d",
            "gitDecoration.modifiedResourceForeground": "#f3bc2e",
            "gitDecoration.deletedResourceForeground": "#c6261e",
            "gitDecoration.untrackedResourceForeground": "#c5d86d"
        }
    },
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "Visual Studio Dark",
    "files.autoSaveDelay": 10000,
    "editor.autoClosingQuotes": "always",
    "editor.autoClosingBrackets": "always",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "explorer.confirmDelete": false,
    "search.showLineNumbers": true,
    "search.useIgnoreFiles": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.windowsEnableConpty": false,
    "http.proxySupport": "off",
    "csharp.suppressProjectJsonWarning": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.maxProjectResults": 100,
    "git.ignoreMissingGitWarning": true,
    "jake.autoDetect": "off",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.compare.location": "scm",
    "gitlens.views.search.location": "scm",
    "gitlens.keymap": "none",
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true,
            "stash": true
        }
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "rest-client.enableTelemetry": false,
    "rest-client.fontSize": 14,
    "editor.formatOnSave": true,
    "explorer.confirmDragAndDrop": false,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always"
}
